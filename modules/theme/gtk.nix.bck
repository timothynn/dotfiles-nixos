{ pkgs, config, ... }:
{
home.packages = with pkgs; [
  # Themes
  catppuccin-gtk

  # Fonts
  nerd-fonts.jetbrains-mono

  # Qt config tool
  libsForQt5.qt5ct
  qt6ct
];
  gtk = {
  enable = true;
  font = {
    name = "JetBrains Mono";
    size = 11;
  };
  theme = {
    name = "Catppuccin-Mocha-Standard-Green-Dark";
    package = pkgs.catppuccin-gtk;
  };
};

}
